{
    "name": "sakshsky/saksh-auth",
    "description": "A lightweight, passwordless OTP-based authentication system for Laravel applications.",
    "keywords": ["laravel", "authentication", "otp", "passwordless"],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "susheel",
        "email": "susheel@sakshsky.com"
      }
    ],
    "require": {
      "php": "^8.1",
      "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
      "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
      "psr-4": {
        "Sakshsky\\SakshAuth\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Sakshsky\\SakshAuth\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Sakshsky\\SakshAuth\\SakshAuthServiceProvider"
        ],
        "aliases": {
          "SakshAuth": "Sakshsky\\SakshAuth\\Facades\\SakshAuth"
        }
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }